{
  "port": 8888,
  "title": "Hello World",
  "trustProxy": true,
  "logging": {
    "format": "combined",
    "options": {}
  },
  "routesDir": "path:./src/routes",
  "middleware": {
    "logger": {
      "priority": 20,
      "module": {
        "name": "morgan",
        "arguments": [
          "config:logging.format",
          "config:logging.options"
        ]
      }
    },
    "body-parser": {
      "priority": 40,
      "module": {
        "name": "body-parser",
        "method": "json"
      }
    },
    "routes": {
      "priority": 100,
      "module": {
        "name": "express-enrouten",
        "arguments": [
          {
            "directory": "config:routesDir"
          }
        ]
      }
    },
    "health-routes": {
      "priority": 101,
      "module": {
        "name": "express-enrouten",
        "arguments": [
          {
            "directory": "path:./build/routes"
          }
        ]
      }
    }
  }
}
